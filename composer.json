{
    "name": "sk9/personal-library",
    "license": "MIT",
    "type": "project",
    "description": "My attempt for a better understanding of CQRS,EventSourcing and PHPSpec",
    "require": {
        "phpspec/phpspec": "2.0.*@dev",
        "symfony/console": "2.6.*@dev",
        "symfony/dependency-injection": "2.6.*@dev",
        "symfony/config": "~2.6",
        "broadway/broadway": "~0.3.0",
        "symfony/proxy-manager-bridge": "~2.3"
    },
    "autoload": {
        "files": [
            "cliApp/bootstrap.php"
        ],
        "psr-4": {
            "Sk9\\PersonalLibrary\\": "src/"
        }
    },
    "config": {
        "process-timeout":      600,
        "preferred-install":    "dist",
        "github-protocols":     ["https"]
    }
}
